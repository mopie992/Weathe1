{
  "expo": {
    "name": "RoadWeather",
    "slug": "roadweather",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
      "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roadweather.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.roadweather.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {}
  }
}

